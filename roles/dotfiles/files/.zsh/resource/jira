# 
# Jira
#

jnew() {
  jira-cli new --type=$1 --priority=Medium --project LE $2 | grep issue | pbcopy;
}

jlist() {
  jira-cli view --search-jql="$1" --oneline
}

jmine() {
  while true
  do
      # load before clear
      output=$(jira mine)
      clear
      echo "$output"
      sleep 60
  done
}

